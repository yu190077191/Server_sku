@using WF.BusinessRule;
@using WF.Framework;
@using WF.Framework.Helper;
@using WF.Model;
@model List<WF.Model.UploadResult>

<table cellpadding="0" cellspacing="0" class="tbl" style="line-height:30px;font-size:14px;">
    @{
        if (Model != null)
        {
            <thead>
                <tr>
                    <th>表名</th>
                    <th>行号</th>
                    <th>名称</th>
                    <th>消息</th>
                </tr>
            </thead>

            foreach (WF.Model.UploadResult item in Model)
            {
                <tr onmouseover="this.className='trFocused'" onmouseout="this.className='trCommon'">
                    <td class="listtd">@(item.TableName)</td>
                    <td class="listtd">@(item.RowNumber)</td>
                    <td class="listtd">@(item.Name)</td>
                    <td class="listtd">@(Html.Raw(item.Message))</td>
                </tr>
            }
        }
    }
</table>